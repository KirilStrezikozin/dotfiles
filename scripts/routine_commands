sudo apt install feh
sudo apt install gcc python3-dev python3-pip libxml2-dev libxslt1-dev zlib1g-dev g++

sudo dnf install gcc-c++
sudo dnf install feh
sudo dnf install picom
sudo dnf install xrandr

# run xrandr for screen names. Command below places Screen1 to the left of the Screen2
xrandr --output Screen2 --auto --pos 0x0 --output Screen1 --auto --left-of Screen2
